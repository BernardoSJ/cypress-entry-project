{
  "name": "cypress-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:reports": "npx --no-install rimraf cypress/reports || true",
    "cy:run": "npx --no-install cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true",
    "cy:report:merge": "npx --no-install mochawesome-merge \"cypress/reports/*-report.json\" > cypress/reports/report.json",
    "cy:report:generate": "npx --no-install marge cypress/reports/report.json --reportDir cypress/reports --reportFilename combined-report.html --inline --overwrite",
    "cy:full": "npm run clean:reports && npm run cy:run && npm run cy:report:merge && npm run cy:report:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "^15.0.0",
    "marge": "^1.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "cypress-xpath": "^2.0.1"
  }
}
